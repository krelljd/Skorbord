{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8000"
      },
      "HttpsEndpoint": {
        "Url": "https://localhost:4300",
        "SslProtocols": [
          "Tls12",
          "Tls13"
        ],
        "Certificate": {
          "Path": "/usr/share/ssl-cert/skorbord.app.combined.pem",
          "KeyPath": "/usr/share/ssl-cert/skorbord.app.key"
        }
      }
    }
  }
}